{
  "version": 2,
  "functions": {
    "api/vercel-ssr/index.js": {
      "includeFiles": "dist/**"
    }
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/api/vercel-ssr/index.js"
    },
    {
      "src": "/proxy",
      "dest": "https://botapi.tamtam.chat/"
    }
  ]
}
